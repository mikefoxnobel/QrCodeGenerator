<ninject:PrismApplication x:Class="QrCodeGenerator.App"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:local="clr-namespace:QrCodeGenerator"
                  xmlns:ninject="http://prismlibrary.com/">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\StyleResource.xaml" />
                <ResourceDictionary Source="Resources\ViewModelResource.xaml" />
                <ResourceDictionary Source="Resources\ConverterResource.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</ninject:PrismApplication>
